-- Scripts:

local function XAGQW_fake_script() -- Minimize.Script 
	local script = Instance.new('Script', Minimize)

	local Mini = script.Parent.Parent.MiniLogo
	Toggle = false
	script.Parent.MouseButton1Click:Connect(function()
		if Toggle == false then
			game:GetService("TweenService"):Create(Mini, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Rotation = 0}):Play();
			script.Parent.Parent.Container.ClipsDescendants = true
			script.Parent.Parent.Container:TweenSize(UDim2.new(0, 194,0, 0), "Out", "Quint", 0.5, true)
			wait(0.001)
			script.Parent.Parent.Container.Visible = false
			Toggle = true
		else
			game:GetService("TweenService"):Create(Mini, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Rotation = 90}):Play();
			script.Parent.Parent.Container.Visible = true
			script.Parent.Parent.Container:TweenSize(UDim2.new(0, 194,0, 256), "Out", "Quint", 0.5, true)
			script.Parent.Parent.Container.ClipsDescendants = false
			Toggle = false
		end
	end)
end
coroutine.wrap(XAGQW_fake_script)()

function dragify(Frame)
dragToggle = nil
dragSpeed = .25 -- You can edit this.
dragInput = nil
dragStart = nil
dragPos = nil

function updateInput(input)
Delta = input.Position - dragStart
Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
game:GetService("TweenService"):Create(Frame, TweenInfo.new(.25), {Position = Position}):Play()
end

Frame.InputBegan:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
dragToggle = true
dragStart = input.Position
startPos = Frame.Position
input.Changed:Connect(function()
if (input.UserInputState == Enum.UserInputState.End) then
dragToggle = false
end
end)
end
end)

Frame.InputChanged:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
dragInput = input
end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
if (input == dragInput and dragToggle) then
updateInput(input)
end
end)
end

dragify(Top)

local Lib = {}

function Lib:Button(name, callback)
    local ButtonContainer = Instance.new("Frame")
    local Button = Instance.new("TextButton")
    local UICorner_2 = Instance.new("UICorner")
    local ButtonName = Instance.new("TextLabel")
    local UIListLayout = Instance.new("UIListLayout")
    local BottomButton = Instance.new("Frame")
    local UICorner_3 = Instance.new("UICorner")
    local callback = callback or function() end
    
    ButtonContainer.Name = "ButtonContainer"
    ButtonContainer.Parent = Container
    ButtonContainer.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    ButtonContainer.BorderSizePixel = 0
    ButtonContainer.Position = UDim2.new(0, 0, NAN, 0)
    ButtonContainer.Size = UDim2.new(0, 194, 0, 48)
    
    Button.Name = "Button"
    Button.Parent = ButtonContainer
    Button.BackgroundColor3 = Color3.fromRGB(17, 27, 34)
    Button.Position = UDim2.new(0.0618556701, 0, 0.1875, 0)
    Button.Size = UDim2.new(0, 171, 0, 30)
    Button.Font = Enum.Font.SourceSans
    Button.Text = ""
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 14.000
    Button.MouseButton1Click:Connect(function()
       pcall(callback) 
    end)
    
    UICorner_2.CornerRadius = UDim.new(0, 4)
    UICorner_2.Parent = Button
    
    ButtonName.Name = "ButtonName"
    ButtonName.Parent = Button
    ButtonName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ButtonName.BackgroundTransparency = 1.000
    ButtonName.Position = UDim2.new(0.187134504, 0, 0.0333333351, 0)
    ButtonName.Size = UDim2.new(0, 107, 0, 28)
    ButtonName.Font = Enum.Font.SourceSans
    ButtonName.Text = name
    ButtonName.TextColor3 = Color3.fromRGB(255, 255, 255)
    ButtonName.TextScaled = true
    ButtonName.TextSize = 14.000
    ButtonName.TextWrapped = true
    
    UIListLayout.Parent = Button
    UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
    
    BottomButton.Name = "BottomButton"
    BottomButton.Parent = ButtonContainer
    BottomButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    BottomButton.BorderColor3 = Color3.fromRGB(20, 20, 20)
    BottomButton.BorderSizePixel = 0
    BottomButton.Position = UDim2.new(0, 0, 0.8125, 0)
    BottomButton.Size = UDim2.new(0, 194, 0, 14)
    
    UICorner_3.Parent = BottomButton
    
    local function HPSBMTC_fake_script() -- ButtonName.Script 
	local script = Instance.new('Script', ButtonName)

	script.Parent.Parent.MouseButton1Click:Connect(function()
		script.Parent:TweenSize(UDim2.new(0, 102,0, 23), "Out", "Quint", 0.2, true)
		wait(0.001)
		script.Parent:TweenSize(UDim2.new(0, 107,0, 28), "Out", "Quint", 0.2, true)
    	end)
    end
    coroutine.wrap(HPSBMTC_fake_script)()
end

function Lib:Toggle(name, callback)
    local ToggleContainer = Instance.new("Frame")
    local BackGroundToggle = Instance.new("Frame")
    local UICorner_4 = Instance.new("UICorner")
    local ToggleBottom = Instance.new("Frame")
    local UICorner_5 = Instance.new("UICorner")
    local ToggleName = Instance.new("TextLabel")
    local Toggle = Instance.new("TextButton")
    local ToggleOFF = Instance.new("ImageButton")
    local ToggleON = Instance.new("ImageButton")
    
    ToggleContainer.Name = "ToggleContainer"
    ToggleContainer.Parent = Container
    ToggleContainer.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    ToggleContainer.BorderSizePixel = 0
    ToggleContainer.Size = UDim2.new(0, 194, 0, 48)
    
    BackGroundToggle.Name = "BackGroundToggle"
    BackGroundToggle.Parent = ToggleContainer
    BackGroundToggle.BackgroundColor3 = Color3.fromRGB(17, 27, 34)
    BackGroundToggle.Position = UDim2.new(0.0567010269, 0, 0.1875, 0)
    BackGroundToggle.Size = UDim2.new(0, 171, 0, 30)
    
    UICorner_4.CornerRadius = UDim.new(0, 4)
    UICorner_4.Parent = BackGroundToggle
    
    ToggleBottom.Name = "ToggleBottom"
    ToggleBottom.Parent = BackGroundToggle
    ToggleBottom.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    ToggleBottom.BorderColor3 = Color3.fromRGB(20, 20, 20)
    ToggleBottom.BorderSizePixel = 0
    ToggleBottom.Position = UDim2.new(-0.0643274859, 0, 0.979166687, 0)
    ToggleBottom.Size = UDim2.new(0, 194, 0, 14)
    
    UICorner_5.Parent = ToggleBottom
    
    ToggleName.Name = "ToggleName"
    ToggleName.Parent = BackGroundToggle
    ToggleName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleName.BackgroundTransparency = 1.000
    ToggleName.Position = UDim2.new(0.0701754391, 0, 0.0333333351, 0)
    ToggleName.Size = UDim2.new(0, 68, 0, 28)
    ToggleName.Font = Enum.Font.SourceSans
    ToggleName.Text = name
    ToggleName.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleName.TextScaled = true
    ToggleName.TextSize = 14.000
    ToggleName.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
    ToggleName.TextWrapped = true
    ToggleName.TextXAlignment = Enum.TextXAlignment.Left
    
    Toggle.Name = "Toggle"
    Toggle.Parent = BackGroundToggle
    Toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Toggle.BackgroundTransparency = 1.000
    Toggle.Position = UDim2.new(0.819000006, 0, 0.0670000017, 0)
    Toggle.Size = UDim2.new(0, 25, 0, 25)
    Toggle.ZIndex = 2
    Toggle.Font = Enum.Font.SourceSans
    Toggle.Text = ""
    Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
    Toggle.TextSize = 14.000
    Toggled = false
    Toggle.MouseButton1Click:Connect(function()
       if Toggled == false then
                ToggleOFF.Visible = false
                ToggleON.Visible = true
                Toggled = true
           else
                ToggleON.Visible = false
                ToggleOFF.Visible = true
                Toggled = false
        end
    end)
    local Toggled2 = false
		Toggle.MouseButton1Click:Connect(function()
			if Toggled2 == false then
				Toggled2 = true
			else
				Toggled2 = false
			end
			callback(Toggled2)
		end)
    
    
    ToggleOFF.Name = "ToggleOFF"
    ToggleOFF.Parent = BackGroundToggle
    ToggleOFF.BackgroundTransparency = 1.000
    ToggleOFF.Position = UDim2.new(0.819000006, 0, 0.0670000017, 0)
    ToggleOFF.Size = UDim2.new(0, 25, 0, 25)
    ToggleOFF.Image = "rbxassetid://3926311105"
    ToggleOFF.ImageRectOffset = Vector2.new(940, 784)
    ToggleOFF.ImageRectSize = Vector2.new(48, 48)
    
    ToggleON.Name = "ToggleON"
    ToggleON.Parent = BackGroundToggle
    ToggleON.BackgroundTransparency = 1.000
    ToggleON.Position = UDim2.new(0.819000006, 0, 0.0670000017, 0)
    ToggleON.Size = UDim2.new(0, 25, 0, 25)
    ToggleON.Visible = false
    ToggleON.Image = "rbxassetid://3926311105"
    ToggleON.ImageRectOffset = Vector2.new(4, 836)
    ToggleON.ImageRectSize = Vector2.new(48, 48)
end

return Lib;

end

