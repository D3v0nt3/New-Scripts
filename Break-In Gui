-- Scripts:

local function PUEJKU_fake_script() -- Home.Script 
	local script = Instance.new('Script', Home)

	local Home = script.Parent
	local Ani = script.Parent.Parent.Parent.HomeAni
	local HomeHub = script.Parent.Parent.Parent.HomeHub
	local ItemHub = script.Parent.Parent.Parent.ItemsHub
	local PlayerHub = script.Parent.Parent.Parent.PlayerHub
	local TpHub = script.Parent.Parent.Parent.TpHub
	
	Home.MouseButton1Click:Connect(function()
		Ani:TweenSize(UDim2.new(0, 301,0, 196), nil, nil, 0.2, nil)
		wait(0.3)
		HomeHub.Visible = true
		ItemHub.Visible = false
		PlayerHub.Visible = false
		TpHub.Visible = false
		Ani:TweenSize(UDim2.new(0, 0,0, 196), nil, nil, 0.2, nil)
	end)
end
coroutine.wrap(PUEJKU_fake_script)()
