	local T2 = script.Parent.Parent.Title2
	
	Exit.MouseButton1Click:Connect(function()
		Exit.Visible = false
		Mini.Visible = false
		T.Visible = false
		T2.Visible = false
		Main:TweenSize(UDim2.new(0, 425,0, 0),nil, nil, 0.3, nil)
		wait()
		Top:TweenSize(UDim2.new(0, 425,0, 0),nil, nil, 0.3, nil)
	end)
end
coroutine.wrap(USGPE_fake_script)()
local function FGUCH_fake_script() -- Minimize.Script 
	local script = Instance.new('Script', Minimize)

	local Mini = script.Parent
	local Main = script.Parent.Parent.MainFrame
	local Toggle = false
	
	Mini.MouseButton1Click:Connect(function()
		if Toggle == false then
			Main:TweenSize(UDim2.new(0, 425,0, 0),nil, nil, 0.3, nil)
			Toggle = true
		else
			Main:TweenSize(UDim2.new(0, 425,0, 215),nil, nil, 0.3, nil)
			Toggle = false
		end
	end)
end
coroutine.wrap(FGUCH_fake_script)()


function dragify(Frame)
dragToggle = nil
dragSpeed = .25 -- You can edit this.
dragInput = nil
dragStart = nil
dragPos = nil

function updateInput(input)
Delta = input.Position - dragStart
Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
game:GetService("TweenService"):Create(Frame, TweenInfo.new(.25), {Position = Position}):Play()
end

Frame.InputBegan:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
dragToggle = true
dragStart = input.Position
startPos = Frame.Position
input.Changed:Connect(function()
if (input.UserInputState == Enum.UserInputState.End) then
dragToggle = false
end
end)
end
end)

Frame.InputChanged:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
dragInput = input
end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
if (input == dragInput and dragToggle) then
updateInput(input)
end
end)
end

dragify(TopFrame)
