-- Scripts:

local function RQMRGP_fake_script() -- Exit.Script 
	local script = Instance.new('Script', Exit)

	local E = script.Parent
	
	E.MouseButton1Click:Connect(function()
		E.BackgroundTransparency = 0
		wait(.3)
		E.BackgroundTransparency = 0.5
	end)
end
coroutine.wrap(RQMRGP_fake_script)()
local function RRPS_fake_script() -- Exit.Script 
	local script = Instance.new('Script', Exit)

	local Main = script.Parent.Parent.MainFrame
	local Top = script.Parent.Parent.Parent.TopFrame
	local Mini = script.Parent.Parent.Minimize
	local T = script.Parent.Parent.Title
	local T2 = script.Parent.Parent.Title2
	local Exit = script.Parent
	
	Exit.MouseButton1Click:Connect(function()
		Main:TweenSize(UDim2.new(0, 490,0, 0), nil, nil, 0.5, nil)
		wait()
		Exit.Visible = false
		Mini.Visible = false
		T.Visible = false
		T2.Visible = false
		Top:TweenSize(UDim2.new(0, 490,0, 0), nil, nil, 0.5, nil)
		wait(0.5)
		Top.Visible = false
	end)
end
coroutine.wrap(RRPS_fake_script)()
local function ARCWS_fake_script() -- Minimize.Script 
	local script = Instance.new('Script', Minimize)

	local E = script.Parent
	
	E.MouseButton1Click:Connect(function()
		E.BackgroundTransparency = 0
		wait(.3)
		E.BackgroundTransparency = 0.5
	end)
end
coroutine.wrap(ARCWS_fake_script)()
local function BPPFT_fake_script() -- Minimize.Script 
	local script = Instance.new('Script', Minimize)

	local Main = script.Parent.Parent.MainFrame
	local Mini = script.Parent
	local Toggle = false
	
	Mini.MouseButton1Click:Connect(function()
		if Toggle == false then
			Main:TweenSize(UDim2.new(0, 490,0, 0), nil, nil, 0.3, nil)
			Toggle = true
		else
			Main:TweenSize(UDim2.new(0, 490,0, 248), nil, nil, 0.3, nil)
			Toggle = false
		end
	end)
end
coroutine.wrap(BPPFT_fake_script)()

function dragify(Frame)
dragToggle = nil
dragSpeed = .25 -- You can edit this.
dragInput = nil
dragStart = nil
dragPos = nil

function updateInput(input)
Delta = input.Position - dragStart
Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
game:GetService("TweenService"):Create(Frame, TweenInfo.new(.25), {Position = Position}):Play()
end

Frame.InputBegan:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
dragToggle = true
dragStart = input.Position
startPos = Frame.Position
input.Changed:Connect(function()
if (input.UserInputState == Enum.UserInputState.End) then
dragToggle = false
end
end)
end
end)

Frame.InputChanged:Connect(function(input)
if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
dragInput = input
end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
if (input == dragInput and dragToggle) then
updateInput(input)
end
end)
end

dragify(TopFrame)

local Lib = {}

function Lib:Button(name, callback)
local TextButton = Instance.new("TextButton")

TextButton.Parent = ButtonContainer
TextButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TextButton.BorderColor3 = Color3.fromRGB(25, 86, 136)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.0250696372, 0, 0.0952381045, 0)
TextButton.Size = UDim2.new(0, 132, 0, 33)
TextButton.Font = Enum.Font.SourceSans
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 20.000
TextButton.Text = name
TextButton.MouseButton1Down:connect(function()
   callback() 
end)
end

